<%= form_for @project do |f| %>
	<%= f.label :name, 'Project Name' %> <%= f.text_field :name%></br>
	<%= f.label :planning_start_date, 'Planning - Start Date' %> <%= f.date_field :planning_start_date %></br>
	<%= f.label :planning_end_date, 'Planning - End Date' %><%= f.date_field :planning_end_date %></br>
	<%= f.label :implementation_start_date, 'Implemenation - Start Date' %> <%= f.date_field :implementation_start_date %></br>
	<%= f.label :implementation_end_date, 'Implementation - End Date' %><%= f.date_field :implementation_end_date %></br>
	<%= f.submit "Submit" %>
<% end %>
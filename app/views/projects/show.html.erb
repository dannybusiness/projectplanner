<div class = 'container-fluid'>
	<div class = 'row'>
		<div class = 'col-lg-4'></div>
		<div class = 'col-lg-4 project'>
			<h3><%= @project.name %></h3>
			<ul>
				<li>Planning Start Date: <%= @project.planning_start_date %></li>
				<li>Planning End Date: <%= @project.planning_end_date %></li>
				<li>Implementation Start Date: <%= @project.implementation_start_date %></li>
				<li>Implementation End Date: <%= @project.implementation_end_date %></li>
			</ul>	
			<div class='btn-group'>
				<%= button_to 'delete', project_path(@project.id), method: :delete, confirm: 'Do you want to delete this project', class: 'btn btn-danger'%>	
				<%= button_to 'edit', edit_project_path(@project.id), method: :get, class: 'btn btn-danger'%>
			</div>
		</div>
		<div class = 'col-lg-4'></div>
	</div>
</div>



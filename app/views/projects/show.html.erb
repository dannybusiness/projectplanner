<h2><%= @project.name %></h2>

<p>Planning Start Date: <%= @project.planning_start_date %></p>
<p>Planning End Date: <%= @project.planning_end_date %></p>
<p>Implementation Start Date: <%= @project.implementation_start_date %></p>
<p>Implementation End Date: <%= @project.implementation_end_date %></p>
<%= button_to 'delete', project_path(@project.id), method: :delete, confirm: 'Do you want to delete this project', class: 'btn btn-danger'%>	
<%= button_to 'edit', edit_project_path(@project.id), method: :get, class: 'btn btn-danger'%>
